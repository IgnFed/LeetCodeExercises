{
	"compilerOptions": {
        "target":"ES6",
        "module":"commonjs",
        "allowUnreachableCode": false,
		"noEmit": false,
        "outDir": "out",
		"sourceMap": true,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"removeComments": false,
	},
	"buildOptions": {
		"verbose": true,
	},
	"compileOnSave": true,
}

